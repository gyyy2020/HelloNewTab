{
  "manifest_version": 3,
  "name": "Hello NewTab",
  "version": "1.0",
  "description": "A Chrome/Firefox extension with a centered search box on new tab",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "96": "icons/icon96.png",
  "128": "icons/icon128.png"
  }
}
